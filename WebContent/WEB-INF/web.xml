<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>pcba</display-name>
  <context-param>
		<param-name>resteasy.scan</param-name>
		<param-value>true</param-value>
	</context-param>
    <servlet>
    <servlet-name>UPDDispatchSerialRestWebservice</servlet-name>
    <servlet-class>
			org.jboss.resteasy.plugins.server.servlet.HttpServletDispatcher
		</servlet-class>
		<init-param>
        <param-name>resteasy.servlet.mapping.prefix</param-name>
        <param-value>/dispatch</param-value>
    </init-param>       
    <init-param>
        <param-name>resteasy.resources</param-name>
        <param-value>com.mot.upd.pcba.restwebservice.UPDDispatchSerialRestWebservice</param-value>
    </init-param>
  </servlet>
  <servlet>
    <servlet-name>UPDSerialSuccessFailureRestWebservice</servlet-name>
    <servlet-class>
			org.jboss.resteasy.plugins.server.servlet.HttpServletDispatcher
		</servlet-class>
		<init-param>
        <param-name>resteasy.servlet.mapping.prefix</param-name>
        <param-value>/successFailure</param-value>
    </init-param>       
    <init-param>
        <param-name>resteasy.resources</param-name>
        <param-value>com.mot.upd.pcba.restwebservice.UPDSerialSuccessFailureRestWebservice</param-value>
    </init-param>
  </servlet>
     <servlet>
    <servlet-name>UPDSWAPUpdateRestWebservice</servlet-name>
    <servlet-class>
			org.jboss.resteasy.plugins.server.servlet.HttpServletDispatcher
		</servlet-class>
		<init-param>
        <param-name>resteasy.servlet.mapping.prefix</param-name>
        <param-value>/swapUpdate</param-value>
    </init-param>       
    <init-param>
        <param-name>resteasy.resources</param-name>
        <param-value>com.mot.upd.pcba.restwebservice.UPDSWAPUpdateRestWebservice</param-value>
    </init-param>
  </servlet>
   <servlet>
    <servlet-name>R12SnSwapUpdateRestWebservice</servlet-name>
    <servlet-class>
			org.jboss.resteasy.plugins.server.servlet.HttpServletDispatcher
		</servlet-class>
		<init-param>
        <param-name>resteasy.servlet.mapping.prefix</param-name>
        <param-value>/swapOldSerialFetchRS</param-value>
    </init-param>       
    <init-param>
        <param-name>resteasy.resources</param-name>
        <param-value>com.mot.upd.pcba.restwebservice.R12SnSwapUpdateRestWebservice</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>UPDSWAPUpdateRestWebservice</servlet-name>
    <url-pattern>/swapUpdate/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UPDDispatchSerialRestWebservice</servlet-name>
    <url-pattern>/dispatch/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UPDSerialSuccessFailureRestWebservice</servlet-name>
    <url-pattern>/successFailure/*</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>R12SnSwapUpdateRestWebservice</servlet-name>
    <url-pattern>/swapOldSerialFetchRS/*</url-pattern>
  </servlet-mapping>
</web-app>